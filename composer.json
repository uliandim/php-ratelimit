{
  "type": "library",
  "name": "jdimitrov/php-ratelimit",
  "description": "PHP rate limiting library with Leaky Bucket Algorithm",
  "require": {
    "php": ">=5.6"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0"
  },
  "suggest": {
    "predis/predis": "^1.1",
    "ext-redis": "^2.2"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Julian Dimitrov",
      "email": "uliandim@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "JDimitrov\\RateLimit\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "JDimitrov\\RateLimit\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "build" : [ "@test" ],
    "test": "@php ./vendor/bin/phpunit"
  }
}
